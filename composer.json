{
    "name": "percymamedy/laravel-dev-booter",
    "description": "Boost your Laravel app by registering Prod services only on Prod.",
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "laravel/framework": "5.0 - 5.4"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "PercyMamedy\\LaravelDevBooter\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestsFixtures\\": "tests/fixtures/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
