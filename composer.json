{
    "name": "percymamedy/laravel-dev-booter",
    "description": "Boost your Laravel app by registering Prod services only on Prod.",
    "keywords": ["php", "Laravel", "dev-tools"],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PercyMamedy\\LaravelDevBooter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestsFixtures\\": "tests/fixtures/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "develop"
        },
        "laravel": {
            "providers": [
                "PercyMamedy\\LaravelDevBooter\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
